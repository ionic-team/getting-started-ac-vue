{
  "name": "getting-started-ac-vue",
  "description": "Auth Connect Vue Tutorial",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "npm run clean && npm run trapeze && npm run build:web && cap sync",
    "build:web": "vue-tsc && vite build",
    "clean": "git checkout android/app/build.gradle android/app/src/main/AndroidManifest.xml ios/App/App/Info.plist",
    "dev": "vite --port=8100",
    "lint": "eslint",
    "prepare": "husky install",
    "preview": "vite preview",
    "test:e2e": "cypress run",
    "test:unit": "vitest run",
    "trapeze": "(export $(cat .env | xargs) && trapeze run -y config.yaml)"
  },
  "dependencies": {
    "@capacitor/android": "^5.0.0",
    "@capacitor/app": "^5.0.0",
    "@capacitor/core": "^5.0.0",
    "@capacitor/haptics": "^5.0.0",
    "@capacitor/ios": "^5.0.0",
    "@capacitor/keyboard": "^5.0.0",
    "@capacitor/status-bar": "^5.0.0",
    "@ionic-enterprise/auth": "~5.0.8",
    "@ionic-enterprise/identity-vault": "~5.12.0",
    "@ionic/vue": "~7.4.1",
    "@ionic/vue-router": "~7.4.1",
    "core-js": "^3.6.5",
    "ionicons": "~7.1.0",
    "vue": "^3.2.21",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.0.0",
    "@ionic/prettier-config": "~4.0.0",
    "@trapezedev/configure": "~7.0.10",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "~12.0.0",
    "@vue/test-utils": "^2.3.0",
    "cypress": "~13.2.0",
    "eslint": "^8.35.0",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "~8.0.2",
    "jsdom": "~22.1.0",
    "prettier": "~2.8.1",
    "pretty-quick": "~3.1.3",
    "typescript": "~5.2.2",
    "vite": "^4.1.0",
    "vitest": "~0.34.5",
    "vue-tsc": "^1.0.24"
  },
  "prettier": "@ionic/prettier-config"
}
